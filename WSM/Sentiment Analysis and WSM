yName":"christian bennett robin","userId":"11935369915464793717"}}},"execution_count":11,"outputs":[]},{"cell_type":"code","source":["import matplotlib.pyplot as plt\n","\n","# Get value counts for speech categories\n","speech_counts = new_df['speech_category'].value_counts()\n","\n","# Create bar plot\n","counts = new_df['speech_category'].value_counts()\n","\n","fig, ax = plt.subplots()\n","counts.plot(kind='bar', ax=ax)\n","\n","ax.set_title('Speech Category Counts')\n","ax.set_xlabel('Speech Category')\n","ax.set_ylabel('Count')\n","\n","plt.show()"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":377},"id":"uawHl71PWIt0","executionInfo":{"status":"ok","timestamp":1678260586886,"user_tz":-420,"elapsed":24,"user":{"displayName":"christian bennett robin","userId":"11935369915464793717"}},"outputId":"705308ec-d916-409b-9c33-84d56f908a8c"},"execution_count":12,"outputs":[{"output_type":"display_data","data":{"text/plain":["<Figure size 432x288 with 1 Axes>"],"image/png":"\n"},"metadata":{"needs_background":"light"}}]},{"cell_type":"markdown","source":["# Categorize tweets with Weighted-Sum Model"],"metadata":{"id":"RrexTnkU-6w4"}},{"cell_type":"code","source":["def classify_tweets(df):\n","    social_weight = 0.67\n","    historical_weight = 0.33\n","    dehumanization_weight = 0.13\n","    accusation_weight = 0.46\n","    attack_weight = 0.06\n","    threat_weight = 0.13\n","    loyalty_weight = 0.22\n","\n","    threshold = 0.5\n","\n","    new_df = pd.DataFrame()\n","    new_df['ID'] = df['ID']\n","    new_df['Tweets'] = df['Tweets']\n","    new_df['clean_tweets'] = df['clean_tweets']\n","    new_df['Social'] = df['social']\n","    new_df['Historical'] = df['historical']\n","    new_df['Dehumanization'] = df['dehumanization']\n","    new_df['Accusation'] = df['accusation']\n","    new_df['Attack'] = df['attack']\n","    new_df['Threat'] = df['threat']\n","    new_df['Loyalty'] = df['loyalty']\n","\n","    severities = []\n","    speech_categories = []\n","    for i in range(len(df)):\n","        social = df['social'][i]\n","        historical = df['historical'][i]\n","        dehumanization = df['dehumanization'][i]\n","        accusation = df['accusation'][i]\n","        attack = df['attack'][i]\n","        loyalty = df['loyalty'][i]\n","        threat = df['threat'][i]\n","\n","        severity = 0.5 * (social * social_weight + historical * historical_weight) + \\\n","                   0.5 * (dehumanization * dehumanization_weight + accusation * accusation_weight + attack * attack_weight + loyalty * loyalty_weight + threat * threat_weight)\n","\n","        if (social == 1 and historical == 0 and dehumanization == 0 and accusation == 0 and attack == 0 and loyalty == 0 and threat == 0) or \\\n","           (social == 0 and historical == 1 and dehumanization == 0 and accusation == 0 and attack == 0 and loyalty == 0 and threat == 0) or \\\n","           (social == 1 and historical == 1 and dehumanization == 0 and accusation == 0 and attack == 0 and loyalty == 0 and threat == 0):\n","            speech_category = 'neutral speech'\n","\n","        elif (social == 0 and historical == 0 and dehumanization == 1 and accusation == 0 and attack == 0 and loyalty == 0 and threat == 0) or \\\n","             (social == 0 and historical == 0 and dehumanization == 0 and accusation == 1 and attack == 0 and loyalty == 0 and threat == 0):\n","            speech_category = 'abusive language'\n","\n","        elif (social == 0 and historical == 0 and dehumanization == 0 and accusation == 0 and attack == 1 and loyalty == 0 and threat == 0) or \\\n","             (social == 0 and historical == 0 and dehumanization == 0 and accusation == 0 and attack == 0 and loyalty == 1 and threat == 0) or \\\n","             (social == 0 and historical == 0 and dehumanization == 0 and accusation == 0 and attack == 0 and loyalty == 0 and threat == 1):\n","            speech_category = 'hate speech'\n","\n","        else:\n","            if severity > threshold:\n","              speech_category = 'dangerous speech'\n","            else:\n","              speech_category = 'hate speech'\n","\n","        severities.append(severity)\n","        speech_categories.append(speech_category)\n","\n","    new_df['severity'] = severities\n","    new_df['speech_category'] = speech_categories\n","\n","    return new_df"],"metadata":{"id":"Urd69yzv-4th","executionInfo":{"status":"ok","timestamp":1678260840287,"user_tz":-420,"elapsed":2,"user":{"displayName":"christian bennett robin","userId":"11935369915464793717"}}},"execution_count":21,"outputs":[]},{"cell_type":"code","source":["df_classified = classify_tweets(df_tweets)\n","df_classified"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":641},"id":"9CfE7JCZ-8CL","executionInfo":{"status":"ok","timestamp":1678260840881,"user_tz":-420,"elapsed":596,"user":{"displayName":"christian bennett robin","userId":"11935369915464793717"}},"outputId":"40f108bc-92fa-4fb9-cdb1-5041701c33da"},"execution_count":22,"outputs":[{"output_type":"execute_result","data":{"text/plain":["                       ID                                             Tweets  \\\n","0     1585739644011892736  Sama banget Komentar ini dgn para pendukung se...   \n","1     1585737538806853632  Ha ha ha sigundul penguasa ancol karena selama...   \n","2     1584780616369049605  Kasihan Ibu ini jadi korban akibat dicuci otak...   \n","3     1584778048120836096  Ha ha hakadrun pada stresssssss mengenai beber...   \n","4     1584774254502912000  Kok sewot dgn Pidato Sambutan Bpk Joko Widodo ...   \n","...                   ...                                                ...   \n","1659  1120875995962101760  @ustadtengkuzul Belajarlah menerima kekalahan,...   \n","1660  1120574450410217472  Aneh tapi Nyata ini bukan HOAX           di Uk...   \n","1661  1119465028040527872  Tim Sukses Pak Jokowi bukan Panglima Hukum, Pr...   \n","1662  1119429279811428353  @FerdinandHutah2 Masih saja kau